<!doctype html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>WIKIO-KART | Final </title>
	<link rel="stylesheet" href="css/score.css">
	<link href='https://fonts.googleapis.com/css?family=Josefin+Sans:400,700' rel='stylesheet' type='text/css'>
</head>
<body>
<section class="front">

</section>

<div id="clouds">
	<div class="centerme congrats">
	<h1>You did it!</h1>
			<br>
			<br>
	<p>Your time was</p>
	</div>
  <div class="cloud x1"></div>
  <div class="cloud x2"></div>
  <div class="cloud x3"></div>
  <div class="cloud x4"></div>
  <div class="cloud x5"></div>

  <div class="centerme">
    {{#each highscores }}
      <h2>{{this.startPage }} --> {{this.endPage }}</h2>
      </br>
      {{#each this.scores }}
        <strong>{{this.name }}</strong>: {{this.time }}
        </br>
      {{/each }}
    {{/each }}
  </div>
</div>

</section>
</body>
</html>
